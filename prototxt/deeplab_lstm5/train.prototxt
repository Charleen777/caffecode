  bottom: "lstm5/h/output/4"
  bottom: "lstm5/h/output/5"
  bottom: "lstm5/h/output/6"
  bottom: "lstm5/h/output/7"
  bottom: "lstm5/h/output/8"
  top: "concat5/h" 
  concat_param {
    axis: 1
  }
}

layer {
  name: "trans/concat5/h"
  type: "Transpose"
  bottom: "concat5/h"
  top: "trans/concat5/h"
  transpose_param {
    transposetype: GRIDLSTM
    batch_size: 1
    local_connected_num: 8
    height: 64
  }
}

layer {
  name: "lstm6/h/input"
  type: "Concat"
  bottom: "trans/concat5/h" 	# 8 connected dimension
  bottom: "lstm5/h/output/0" 	# depth dimension
  top: "lstm6/h/input"
  concat_param {
    axis: 1
  }
}

layer {
  name: "score5"
  type: "Transpose"
  bottom: "lstm6/h/input"
  top: "score5"
  transpose_param {
    transposetype: LSTMOUTPUT
    batch_size: 1
    height: 64
  }
}

layer {
  name: "feedforward5"
  type: "Convolution"
  bottom: "score5"
  top: "feedforward5"
  param {
    lr_mult: 5
    decay_mult: 1
  }
  param {
    lr_mult: 10
    decay_mult: 0
  }
  convolution_param {
    num_output: 3
    kernel_size:1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  } 
}

layer {
  name: "accuracy5"
  type: "SegAccuracy"
  bottom: "feedforward5"
  bottom: "label_shrink"
  top: "accuracy5"
  seg_accuracy_param {
    ignore_label: 3
  } 
}
layer {
  name: "loss5"
  type: "SoftmaxWithLoss"
  bottom: "feedforward5"
  bottom: "label_shrink"
  top: "loss5"
  loss_param {
    ignore_label: 3
  }
  include: { phase: TRAIN }
}



