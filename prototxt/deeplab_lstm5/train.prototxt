name: "deeplab-LSTM5"

########################################################
#
#      Data Input
#
########################################################


layer{
  name: "data"
  type: "ImageSegData" 
  top: "data"
  top: "label"
  image_data_param {
    root_folder: "../dataset/SiftFlowDataset/"
    source: "../dataset/SiftFlowDataset/trainList.txt"
    label_type: PIXEL
    batch_size: 1 #16 #30
    shuffle: false
  }
  transform_param {
    mean_value: 108.5138  # SiftFlowDataset
    mean_value: 115.3204
    mean_value: 114.9225
    crop_size: 0
    mirror: false
  }
  include: { phase: TRAIN }
}
layer{
  name: "data"
  type: "ImageSegData" 
  top: "data"
  top: "label"
  image_data_param {
    root_folder: "../dataset/SiftFlowDataset/"
    source: "../dataset/SiftFlowDataset/trainList.txt"
    label_type: PIXEL
    batch_size: 1 #16 #30
  }
  transform_param {
    mean_value: 108.5138  # SiftFlowDataset
    mean_value: 115.3204
    mean_value: 114.9225
    crop_size: 0
    mirror: false
  }
  include: { phase: TEST }
}

layer {
  bottom: "label"
  top: "label_shrink"
  name: "label_shrink"
  type: "Interp"
  interp_param {
    shrink_factor: 4
    pad_beg: 0
    pad_end: 0
  }
}
########################################################
#
#      Feature Extraction via deeplab
#
########################################################

layer {
  bottom: "data"
  top: "conv1_1"
  name: "conv1_1"
  type: "Convolution"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult:2
    decay_mult:0
  }
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
  }
}
layer {
  bottom: "conv1_1"
  top: "conv1_1"
  name: "relu1_1"
  type: "ReLU"
}
layer {
  bottom: "conv1_1"
  top: "conv1_2"
  name: "conv1_2"
  type: "Convolution"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult:2
    decay_mult:0
  }
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
  }
}
layer {
  bottom: "conv1_2"
  top: "conv1_2"
  name: "relu1_2"
  type: "ReLU"
}
layer {
  bottom: "conv1_2"
  top: "pool1"
  name: "pool1"
  type: "Pooling"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
    pad: 1
  }
}
layer {
  bottom: "pool1"
