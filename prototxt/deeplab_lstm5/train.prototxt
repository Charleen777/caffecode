    transposetype: GRIDLSTM
    batch_size: 1
    local_connected_num: 8
    height: 64
  }
}

layer {
  name: "lstm3/h/input"
  type: "Concat"
  bottom: "trans/concat2/h" 	# 8 connected dimension
  bottom: "lstm2/h/output/0" 	# depth dimension
  top: "lstm3/h/input"
  concat_param {
    axis: 1
  }
}

layer {
  name: "score2"
  type: "Transpose"
  bottom: "lstm3/h/input"
  top: "score2"
  transpose_param {
    transposetype: LSTMOUTPUT
    batch_size: 1
    height: 64
  }
}

layer {
  name: "feedforward2"
  type: "Convolution"
  bottom: "score2"
  top: "feedforward2"
  param {
    lr_mult: 5
    decay_mult: 1
  }
  param {
    lr_mult: 10
    decay_mult: 0
  }
  convolution_param {
    num_output: 3
    kernel_size:1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  } 
}

layer {
  name: "accuracy2"
  type: "SegAccuracy"
  bottom: "feedforward2"
  bottom: "label_shrink"
  top: "accuracy2"
  seg_accuracy_param {
    ignore_label: 3
  } 
}
layer {
  name: "loss2"
  type: "SoftmaxWithLoss"
  bottom: "feedforward2"
  bottom: "label_shrink"
  top: "loss2"
  loss_param {
    ignore_label: 3
  }
  include: { phase: TRAIN }
}



