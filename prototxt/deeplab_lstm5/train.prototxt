  bottom: "pool5a"
  top: "fc6"
  name: "fc6"
  type: "Convolution"
#  strict_dim: false
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult:2
    decay_mult:0
  }
  convolution_param {
    num_output: 1024
    #pad: 12
    #dilation: 12
    kernel_size: 3
  }
}
layer {
  bottom: "fc6"
  top: "fc6"
  name: "relu6"
  type: "ReLU"
}
layer {
  bottom: "fc6"
  top: "fc6"
  name: "drop6"
  type: "Dropout"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  bottom: "fc6"
  top: "fc7"
  name: "fc7"
  type: "Convolution"
#  strict_dim: false
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult:2
    decay_mult:0
  }
  convolution_param {
    num_output: 1024
    kernel_size: 1
  }
}
layer {
  bottom: "fc7"
  top: "fc7"
  name: "relu7"
  type: "ReLU"
}
layer {
  bottom: "fc7"
  top: "fc7"
  name: "drop7"
  type: "Dropout"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "upsample/fc7"
  type: "Deconvolution"
  bottom: "fc7"
  top: "upsample/fc7"
  convolution_param {
    kernel_size: 4		#{{2 * factor - factor % 2}}
    stride: 2			#{{factor}}
    num_output: 1024   		#{{C}}
    pad: 6			#{{ceil((factor - 1) / 2.)}}
    bias_term: false
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}


########################################################
#
#      Transition Layer
#
########################################################

layer {
  name: "lstm_input"
  type: "Transpose"
  bottom: "upsample/fc7"
  top: "lstm_input"
  transpose_param {
    transposetype: LSTMINPUT
  }
}
layer {
  name: "zero_data"
  type: "ZerosData"
  top: "lstm_mem_seed"
  zeros_data_param {
    shape: 4096  #batch number
    shape: 64
  }
}

layer {
  bottom: "lstm_input"
  bottom: "lstm_mem_seed"
  top: "lstm0/h/0"
  top: "lstm0/m/0"
  name: "lstm0/0"
  type: "LstmUnit"
  param{
     lr_mult: 10
  }
  param{
     lr_mult: 10
  }
  param{
     lr_mult: 10
  }
  param{
     lr_mult: 10
  }
  lstm_unit_param {
    num_cells: 64
    input_weight_filler {
      type: "uniform"
      min: -0.01
      max: 0.01
    }
    input_gate_weight_filler {
      type: "uniform"
      min: -0.01
      max: 0.01    
    }
    forget_gate_weight_filler {
      type: "uniform"
      min: -0.01
      max: 0.01
    }
    output_gate_weight_filler {
      type: "uniform"
      min: -0.01
      max: 0.01      
    }
  }
}

layer {
  name: "lstm0/0/drop"
  type: "Dropout"
  bottom: "lstm0/h/0"
  top: "lstm0/h/0"
  dropout_param {
     dropout_ratio: 0.5
  }
}


