train_net: "prototxt/deeplab_lstm1/train.prototxt"

lr_policy: "step"
gamma: 0.1
stepsize: 2000
base_lr: 0.01

display: 1
max_iter: 1000000 #7000000
momentum: 0.9
weight_decay: 0.0005

snapshot: 1000
snapshot_prefix: "snapshots/deeplab_lstm1"
solver_mode: GPU
#debug_info: true
